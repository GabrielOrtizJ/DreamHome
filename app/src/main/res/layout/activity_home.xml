<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo_beta2"
    tools:context=".HomeActivity">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:background="@drawable/logo_redondo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="339dp"
        android:layout_height="54dp"
        android:layout_marginBottom="4dp"
        android:background="@android:drawable/divider_horizontal_textfield"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:titleTextColor="@color/cardview_shadow_end_color" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="403dp"
        android:layout_height="37dp"
        android:layout_marginBottom="68dp"
        android:background="@android:drawable/divider_horizontal_textfield"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/EmailTextView"
            android:layout_width="226dp"
            android:layout_height="24dp"
            android:gravity="center"
            android:hint="Email"
            android:textColor="@color/white"
            android:textColorHint="@color/common_google_signin_btn_text_dark_disabled" />

        <TextView
            android:id="@+id/providerTextView"
            android:layout_width="124dp"
            android:layout_height="24dp"
            android:layout_marginEnd="4dp"
            android:layout_marginBottom="112dp"
            android:gravity="center"
            android:hint="provider"
            android:textColor="@color/white"
            android:textColorHint="@color/common_google_signin_btn_text_dark_disabled" />

    </LinearLayout>

    <SearchView
        android:id="@+id/searchView"
        android:layout_width="285dp"
        android:layout_height="55dp"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="240dp"
        android:background="@android:drawable/button_onoff_indicator_off"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="221dp"
        android:layout_height="72dp"
        android:layout_marginTop="20dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:text="Buscar propiedades"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <LinearLayout
        android:layout_width="191dp"
        android:layout_height="86dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchView">

        <TextView
            android:id="@+id/Buscar_contactos_button"
            android:layout_width="58dp"
            android:layout_height="53dp"
            android:layout_margin="10dp"
            android:background="@drawable/buscar_usuario" />

        <TextView
            android:id="@+id/btnAddHome"
            android:layout_width="58dp"
            android:layout_height="53dp"
            android:layout_margin="10dp"
            android:background="@drawable/casa"
            android:padding="10dp" />
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>