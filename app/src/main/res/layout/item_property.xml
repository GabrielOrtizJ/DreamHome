<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/MaterialCardView"
    android:layout_width="430dp"
    android:layout_height="140dp"
    android:layout_marginTop="15dp"
    app:cardCornerRadius="80px">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="472dp"
        android:layout_height="173dp"
        android:background="?attr/actionBarDivider">

        <TextView
            android:id="@+id/precio"
            android:layout_width="123dp"
            android:layout_height="19dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="48dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="1000000"
            android:textColor="#00C300"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/direccionItem" />

        <TextView
            android:id="@+id/direccionItem"
            android:layout_width="283dp"
            android:layout_height="38dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="6dp"
            android:layout_marginBottom="116dp"
            android:fontFamily="@font/alfa_slab_one"
            android:gravity="center_vertical"
            android:text="Direccion"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.652"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="124dp"
            android:layout_marginBottom="92dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="Hab"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="236dp"
            android:layout_marginBottom="92dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="Baños"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/bañosItem"
            android:layout_width="35dp"
            android:layout_height="0dp"
            android:layout_marginEnd="184dp"
            android:layout_marginBottom="92dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/habItem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="292dp"
            android:layout_marginBottom="92dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/asesorNombreItem"
            android:layout_width="131dp"
            android:layout_height="29dp"
            android:layout_marginEnd="208dp"
            android:layout_marginBottom="64dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="AsesorNombre"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/precios"
            android:layout_width="14dp"
            android:layout_height="25dp"
            android:layout_marginEnd="4dp"
            android:layout_marginBottom="88dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="€"
            android:textColor="#00C300"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/precio" />

        <TextView
            android:id="@+id/descripcionItem"
            android:layout_width="122dp"
            android:layout_height="29dp"
            android:layout_marginStart="300dp"
            android:layout_marginTop="8dp"
            android:text="descripcion"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/idP" />

        <TextView
            android:id="@+id/textView28"
            android:layout_width="99dp"
            android:layout_height="78dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/baseline_home_24"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/idP"
            android:layout_width="203dp"
            android:layout_height="20dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="144dp"
            android:text="id"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/asesorNombreItem" />

        <TextView
            android:id="@+id/tipo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="1dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="tipo"
            android:textColor="@color/teal_700"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/textView28"
            app:layout_constraintStart_toStartOf="@+id/textView28"
            app:layout_constraintTop_toBottomOf="@+id/textView28" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</com.google.android.material.card.MaterialCardView>
