<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/MaterialCardView"
    android:layout_width="430dp"
    android:layout_height="100dp"
    android:layout_marginTop="15dp"
    app:cardCornerRadius="80px">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="472dp"
        android:layout_height="118dp"
        android:background="?attr/actionBarDivider">

        <TextView
            android:id="@+id/precio"
            android:layout_width="123dp"
            android:layout_height="19dp"
            android:layout_marginEnd="48dp"
            android:layout_marginBottom="24dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="1000000"
            app:layout_constraintBottom_toTopOf="@+id/idP"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/direccionItem"
            android:layout_width="262dp"
            android:layout_height="32dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="6dp"
            android:layout_marginBottom="76dp"
            android:fontFamily="@font/alfa_slab_one"
            android:gravity="center_vertical"
            android:text="Direccion"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.553"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/bañosItem"
            android:layout_width="35dp"
            android:layout_height="0dp"
            android:layout_marginEnd="196dp"
            android:layout_marginBottom="56dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="248dp"
            android:layout_marginBottom="56dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="Baños"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="120dp"
            android:layout_marginBottom="56dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="Hab"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/habItem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="304dp"
            android:layout_marginBottom="56dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/asesorNombreItem"
            android:layout_width="131dp"
            android:layout_height="29dp"
            android:layout_marginEnd="220dp"
            android:layout_marginBottom="28dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="AsesorNombre"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/descripcionItem"
            android:layout_width="122dp"
            android:layout_height="29dp"
            android:layout_marginTop="72dp"
            android:layout_marginEnd="16dp"
            android:text="descripcion"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/direccionItem" />

        <TextView
            android:id="@+id/textView28"
            android:layout_width="81dp"
            android:layout_height="57dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/baseline_home_24"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/idP"
            android:layout_width="203dp"
            android:layout_height="20dp"
            android:layout_marginEnd="52dp"
            android:layout_marginBottom="12dp"
            android:text="id"
            app:layout_constraintBottom_toTopOf="@+id/descripcionItem"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/tipo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="1dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="tipo"
            android:textColor="@color/teal_700"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/textView28"
            app:layout_constraintStart_toStartOf="@+id/textView28"
            app:layout_constraintTop_toBottomOf="@+id/textView28" />

        <TextView
            android:id="@+id/precios"
            android:layout_width="9dp"
            android:layout_height="19dp"
            android:layout_marginEnd="172dp"
            android:layout_marginBottom="24dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="€"
            app:layout_constraintBottom_toTopOf="@+id/idP"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</com.google.android.material.card.MaterialCardView>
