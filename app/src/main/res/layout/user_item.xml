<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/MaterialCardView"
    android:layout_width="430dp"
    android:layout_height="140dp"
     android:rotation="-360"
    android:layout_marginTop="7dp"
    android:layout_marginLeft="7dp"
    app:cardCornerRadius="100px">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="472dp"
        android:layout_height="173dp"
        android:background="?attr/actionBarDivider"
        android:rotation="-360"
        android:rotationX="-360"
        android:rotationY="-360">


        <TextView
            android:id="@+id/nombre"
            android:layout_width="283dp"
            android:layout_height="38dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="6dp"
            android:layout_marginBottom="116dp"
            android:fontFamily="@font/alfa_slab_one"
            android:gravity="center_vertical"
            android:text="Nombre"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.646"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="124dp"
            android:layout_marginBottom="92dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="Direccion"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="124dp"
            android:layout_marginBottom="68dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="TLF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/tlf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginBottom="68dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="XXX XXX XXX"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView18" />

        <TextView
            android:id="@+id/address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:layout_marginBottom="92dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="EspaÃ±a"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView16" />

        <TextView
            android:id="@+id/tipo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="tipo"
            android:textColor="@color/teal_700"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/imageUserlist"
            app:layout_constraintStart_toStartOf="@+id/imageUserlist"
            app:layout_constraintTop_toBottomOf="@+id/imageUserlist" />

        <ImageView
            android:id="@+id/imageUserlist"
            android:layout_width="103dp"
            android:layout_height="86dp"
            android:layout_marginStart="15dp"
            android:layout_marginTop="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/perfil" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</com.google.android.material.card.MaterialCardView>
